{
  "name": "envision",
  "version": "0.0.1-alpha",
  "description": "SiteVision CSS framework",
  "main": "index.js",
  "scripts": {
    "build-docs": "cd _docs/; jekyll build; touch ../docs/.nojekyll",
    "build-docs-js": "webpack --config webpack.docs.js",
    "build-dist": "webpack --config webpack.dist.js",
    "build": "npm run build-docs-js && npm run build-docs && npm run build-dist",
    "watch": "webpack --config webpack.docs.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sitevision/envision.git"
  },
  "keywords": [
    "Envision",
    "CSS",
    "SiteVision",
    "SDK"
  ],
  "author": "SiteVision AB",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "autoprefixer": "^9.7.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "eslint": "^6.6.0",
    "eslint-loader": "^3.0.2",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.3.1",
    "terser-webpack-plugin": "^2.3.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "popper.js": "^1.16.0",
    "svgxuse": "^1.2.6"
  },
  "browserslist": [
    "Chrome >= 45",
    "Firefox ESR",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ]
}
